#LIST:

a=[]
a=a+[5,6,7,8,10]
a=a+[8,3,4,4,10,9,1,5,6,7,8,1]
print(a)
print(a.count(8))
print(a.index(10))
print(sum(a)/2)
print(sum(a)+min(a)+max(a))
a.sort()
print(a)
print(" median:")
mid=int(len(a)/2)  
print(a[mid])  
print(tuple(set(a)))
print("list to set:",set(a)) 
print("list to tuple:",tuple(a))

