#Tuple

a=(1,4,5,6,7,8)
b=(5,6,7,8,9)
print("common elements")
c=tuple(set(a) & set(b))
print(c)
c=(a+b)
k=tuple(set(c)) 
print("concatenation result witout duplicates:")
print(k)
print("index of element 9")
print(k.index(9))
print("multiply elements 3 times")
k=k*3
print(k)

O/P
